<%= form_with model: [ @room, @room.bookings.build ] do |form| %>
  <p>
    <%= form.label :guest %><br>
    <%= form.text_field :guest %>
  </p>
  <p>
    <%= form.label :start_date %><br>
    <%= form.date_field :start_date %>
  </p>
  <p>
    <%= form.label :end_date %><br>
    <%= form.date_field :end_date %>
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>